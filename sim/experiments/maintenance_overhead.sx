defaults {
  # Node
  'node.maintenance_algorithm' = Maintenance::PushPull,
  'node.maintenance_opportunistic_alwayson' => true,
  'node.maintenance_size' => 5,
  'node.maintenance_rate' => 1000,

  # Link Table
  'link_table.max_peers' => 15,
  'link_table.address_space' => 10000,

  # Analyzer
  'analyzer.measurement_period' => 10000,
  'analyzer.output_path' => 'output'
}

new_experiment 'maintenance_overhead_search_rates'
set 'workload', ['1000_mxt480000_s-all-2000.wl.gz', '1000_mxt480000_s-all-20000.wl.gz', '1000_mxt480000_s-all-40000.wl.gz', '1000_mxt480000_s-all-120000.wl.gz']

log 'link_table.max_peers', 32, 96, 1.5
static 'length', 480000 + 5 * 60000

# Bamboo used searches ever 10000 ticks
new_experiment 'maintenance_overhead_churn_rates'
set 'workload', ['1000_mxt60000_s-all-20000.wl.gz', '1000_mxt120000_s-all-20000.wl.gz', '1000_mxt240000_s-all-20000.wl.gz', '1000_mxt480000_s-all-20000.wl.gz', '1000_mxt960000_s-all-20000.wl.gz', '1000_mxt1920000_s-all-20000.wl.gz', '1000_mxt3840000_s-all-20000.wl.gz']

static 'link_table.max_peers', 64
static 'length', 1920000 + 5 * 60000
